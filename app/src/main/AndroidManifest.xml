<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wheel.chart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wheel.chart.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.wheel.chart.activity.CombinedChartActivity"/>
        <activity android:name="com.wheel.chart.activity.LineChartActivity"/>
        <activity android:name="com.wheel.chart.activity.DynamicLineChartActivity"/>
        <activity android:name="com.wheel.chart.activity.BarChartActivity"/>
        <activity android:name="com.wheel.chart.activity.RadarChartActivity"/>
        <activity android:name="com.wheel.chart.activity.PieChartActivity"/>
        <activity android:name="com.wheel.chart.activity.MeterActivity"/>
    </application>

</manifest>